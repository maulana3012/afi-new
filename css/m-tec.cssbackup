	border-radius: 8px;
	width: 850px;
	height:150px;
	-webkit-font-size: 18px;
	-webkit-border-radius: 8px;
}

.btn-call {
 width: 200px;
 height: 50px;
}

.btn-web {
 width: 200px;
 height: 50px;
}



