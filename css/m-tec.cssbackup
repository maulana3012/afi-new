	height: 100%;
	width: calc(100vh * 2/12);
	float: left;
	padding: 10px;
}

.navbar-header {
	float: left;
}

.footer {
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	background-color: #003366;
	color: #FFFFFF;
	text-align: center;
	height:8em;	
}

.fo 	{
        margin-top: -90px;
	height: 100%;
	width: calc(100vh * 7/9);

}

button.btn {
	background: linear-gradient(-600deg, #FFFFFF 88%, #003366 12%);
