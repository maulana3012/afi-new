	background-position: center top;
	background-size: cover;
}
.main-axa {
		padding: 0 10px;
		width: 750px;
		margin: 0 -60px;
}

.navbar-axa {
	margin-top:-70px;
	background-color: white;
	border-radius: none;
	border: none;
}

